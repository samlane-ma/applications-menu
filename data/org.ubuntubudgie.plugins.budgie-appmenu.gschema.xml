<schemalist>
	<schema path="/org/ubuntubudgie/plugins/budgie-appmenu/" id="org.ubuntubudgie.plugins.budgie-appmenu">
        <key name="rows" type="i">
            <default>3</default>
            <range min="2" max="10"/>
            <summary>Number of rows for the grid view to display.</summary>
        </key>
        <key name="columns" type="i">
            <default>5</default>
            <range min="2" max="10"/>
            <summary>Number of columns for the grid view to display.</summary>
        </key>
        <key name="enable-powerstrip" type="b">
            <default>false</default>
            <summary>Show Power-strip</summary>
        </key>
        <key name="rollover-menu" type="b">
            <default>false</default>
            <summary>Allow category view rollover menu</summary>
        </key>
	</schema>
</schemalist>
